#!/bin/bash

for i in *.base
do
    last_file="$i"
    final_file="${i%.base}"

    for stage in ${final_file}.*
    do
	# Strip out emacs junk
	if [[ $stage == *~ ]]; then
	    continue
	fi
	
	# We are at the end!
	if [[ $stage == *.base ]]; then
	    stage=$final_file
	fi

	echo Need to patch $last_file to make $stage

	last_file=$stage
    done
    echo
done
